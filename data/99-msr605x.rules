# udev rules for MSR605X Magnetic Stripe Card Reader/Writer
# Install to /etc/udev/rules.d/99-msr605x.rules

# MSR605X USB HID device
SUBSYSTEM=="usb", ATTR{idVendor}=="0801", ATTR{idProduct}=="0003", MODE="0666", GROUP="plugdev", TAG+="uaccess"
SUBSYSTEM=="hidraw", ATTRS{idVendor}=="0801", ATTRS{idProduct}=="0003", MODE="0666", GROUP="plugdev", TAG+="uaccess"

# Alternative vendor IDs (some clones may use different IDs)
SUBSYSTEM=="usb", ATTR{idVendor}=="0801", ATTR{idProduct}=="0002", MODE="0666", GROUP="plugdev", TAG+="uaccess"
SUBSYSTEM=="hidraw", ATTRS{idVendor}=="0801", ATTRS{idProduct}=="0002", MODE="0666", GROUP="plugdev", TAG+="uaccess"
